<% ulimits = scope.lookupvar('slurm::service_ulimits') -%>
<% if ulimits -%>
<% ulimits.each do |ulimit| -%>
ulimit <%= ulimit %>
<% end -%>
<% end -%>

CONFDIR="<%= scope.lookupvar('slurm::conf_dir') %>"
SLURMCTLD_OPTIONS="-f <%= scope.lookupvar('slurm::slurm_conf_path') %>"
SLURMD_OPTIONS="-f <%= scope.lookupvar('slurm::slurm_conf_path') %> -M"
export SLURM_CONF="<%= scope.lookupvar('slurm::slurm_conf_path') %>"
